            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });

        wx.previewImage({
            current: 'http://180.166.144.198/Emppa/Images/YY.png', // 当前显示图片的http链接
            urls: ['http://180.166.144.198/Emppa/Images/YY.png'] // 需要预览的图片http链接列表
        });
    });
</script>

